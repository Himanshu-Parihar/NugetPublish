<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <PackageId>Harry.Pubish.nuget</PackageId>
    <Authors>Himanshu</Authors>
    <Description>A password box that allows binding.</Description>
    <PackageTags>password box wpf binding mvvm</PackageTags>
    <RepositoryUrl>https://github.com/Himanshu-Parihar/NugetPublish/PasswordBoxMVVM</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

</Project>
